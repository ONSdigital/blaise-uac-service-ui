    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.15",
    "@types/multer": "^1.4.7",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "axios": "^0.21.1",
    "blaise-design-system-react-components": "git+https://github.com/ONSdigital/blaise-design-system-react-components#v0.5.3",
    "body-parser": "^1.18.3",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "google-auth-library": "^7.6.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.3",
    "pino": "^6.13.0",
    "react": "^17.0.2",
    "react-csv-downloader": "^2.7.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "scramjet": "^4.35.20",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@jest-mock/express": "^1.4.3",
    "@types/express-list-endpoints": "^4.0.1",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/supertest": "^2.0.11",
    "axios-mock-adapter": "^1.19.0",
    "concurrently": "^4.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "express-list-endpoints": "^6.0.0",
    "supertest": "^6.1.4"
